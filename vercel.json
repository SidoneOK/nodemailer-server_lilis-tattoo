{
    "version": 2,
    "env": {},
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "app.js" }
    ],
        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "https://yuratattoodo.com" }
            ]
          }
        ]
}

